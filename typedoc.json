{
	"$schema": "https://typedoc.org/schema.json",
	"entryPoints": [ "src/index.ts" ],
	"tsconfig": "tsconfig.json",

	"out": "docs",
	"name": "power-queues API Docs",
	"readme": "README.md",
	"includeVersion": true,

	"exclude": [ "**/*.spec.ts", "**/dist/**", "**/*.js" ],
	"excludeExternals": true,
	"excludeInternal": true,
	"excludePrivate": false,
	"excludeNotDocumented": false,

	"categorizeByGroup": true,
	"searchInComments": true,

	"plugin": [],
	"githubPages": false,

	"highlightLanguages": [ "ts", "js", "json", "bash", "regex", "lua" ],

	"visibilityFilters": {
		"protected": true,
		"private": true,
		"inherited": true,
		"external": true
	}
}